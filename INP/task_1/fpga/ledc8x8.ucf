# SMCLK je hlavni hodinovy vstup,
# zde pro nej specifikujeme kmitocet
NET SMCLK TNM_NET = smclk_pin;
TIMESPEC TS_smclk_pin = PERIOD smclk_pin 8 MHz;
# ...a samozrejme priradime pin na FPGA
NET SMCLK      LOC = P80;

